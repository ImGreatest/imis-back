{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "plugins": [
      "@nestjs/swagger"
    ],
    "deleteOutDir": true
  },
  "projects": {
    "cabinet": {
      "type": "application",
      "root": "apps/cabinet",
      "sourceRoot": "apps/cabinet/src",
      "entryFile": "main",
      "compilerOptions": {
        "tsConfigPath": "apps/cabinet/tsconfig.app.json"
      }
    },
    "rating": {
      "type": "application",
      "root": "apps/rating",
      "sourceRoot": "apps/rating/src",
      "entryFile": "main",
      "compilerOptions": {
        "tsConfigPath": "apps/rating/tsconfig.app.json"
      }
    },
    "mobile": {
      "type": "application",
      "root": "apps/mobile",
      "sourceRoot": "apps/mobile/src",
      "entryFile": "main",
      "compilerOptions": {
        "tsConfigPath": "apps/mobile/tsconfig.app.json"
      }
    },
    "employer": {
      "type": "application",
      "root": "apps/employer",
      "sourceRoot": "apps/employer/src",
      "entryFile": "main",
      "compilerOptions": {
        "tsConfigPath": "apps/employer/tsconfig.app.json"
      }
    }
  }
}
